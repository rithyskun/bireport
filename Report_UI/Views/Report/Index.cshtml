@model IEnumerable<Report_DO.MasterReport>

@{
    ViewBag.Title = "Index";
}


<div class="container">
    <div class="col-md-12">
        <h2>Manage Master Report</h2>
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table table-striped table-hover">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.MasterReportListID)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.MasterReportListName)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.MasterReportListID)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MasterReportListName)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.MasterReportListID }) |
                        @Html.ActionLink("Details", "Details", new { id = item.MasterReportListID }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.MasterReportListID })
                    </td>
                </tr>
            }

        </table>
    </div>
</div>