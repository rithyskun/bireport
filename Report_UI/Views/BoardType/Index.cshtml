@model IEnumerable<Report_DO.BoardType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")&nbsp;&nbsp;&nbsp;&nbsp;
    @Html.ActionLink("Go back System", "System", "Home")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BSCNameID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubRackNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SlotNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Boardtype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LogicalFunctionType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BSCNameID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubRackNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SlotNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Boardtype)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LogicalFunctionType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BoardTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.BoardTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BoardTypeId })
        </td>
    </tr>
}

</table>
